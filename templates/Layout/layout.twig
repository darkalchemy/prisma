    <![endif]-->
    {{ assets({files: [
        '@public/css/d.css',
        '@public/css/notifIt-app.css',
        'Layout/layout.css',
        'Layout/print.css'
        ], name: 'layout.css'})
    }}
    {{ assets({files: [
        '@public/js/d.js',
        '@public/js/notifIt.js',
        'Layout/app.js'
        ], name: 'layout.js'})
    }}
    {% block assets %}{% endblock %}
    {% autoescape false %}
    <script>$d.setText({{ text|json_encode(constant('JSON_HEX_QUOT') b-or constant('JSON_HEX_APOS'))|raw }})</script>
    {% endautoescape %}
</head>
<body>
